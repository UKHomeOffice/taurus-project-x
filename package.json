{
  "name": "clinics-test",
  "version": "1.0.0",
  "description": "home office interview: clinics test",
  "main": "index.js",
  "scripts": {
    "start": "cp config.local.json config.json && node dist/index.js",
    "test": "cp config.test.json config.json && npm run build:test && mocha",
    "build:test":       "./node_modules/babel-cli/bin/babel.js ./src --out-dir ./dist/src --quiet && ./node_modules/babel-cli/bin/babel.js ./index.js -o ./dist/index.js --quiet && cp config.json ./dist/config.json",
    "build": "./node_modules/babel-cli/bin/babel.js ./src --out-dir ./dist/src --ignore **/*.spec.js --quiet && ./node_modules/babel-cli/bin/babel.js ./index.js -o ./dist/index.js --quiet && cp config.json ./dist/config.json"
  },
  "repository": {
    "type": "git",
    "url": "placeholder"
  },
  "author": "Robert Dabin",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "chai": "^3.5.0",
    "mocha": "^3.3.0",
    "sinon": "^2.1.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "good": "^7.1.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.1.1",
    "lodash": "^4.17.4",
    "request": "^2.81.0",
    "request-promise": "^4.2.0"
  },
  "babel": {
    "presets": [
      [
        "env"
      ]
    ],
    "ignore": [
      "node_modules/"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  }
}
